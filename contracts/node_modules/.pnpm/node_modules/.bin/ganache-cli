#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/ganache@7.9.2/node_modules/ganache/dist/node/node_modules:/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/ganache@7.9.2/node_modules/ganache/dist/node_modules:/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/ganache@7.9.2/node_modules/ganache/node_modules:/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/ganache@7.9.2/node_modules:/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/ganache@7.9.2/node_modules/ganache/dist/node/node_modules:/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/ganache@7.9.2/node_modules/ganache/dist/node_modules:/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/ganache@7.9.2/node_modules/ganache/node_modules:/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/ganache@7.9.2/node_modules:/home/samuel/Dev/work/hackathon/zeroai-mu-hack/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ganache/dist/node/cli.js" "$@"
else
  exec node  "$basedir/../ganache/dist/node/cli.js" "$@"
fi
